{
  "name": "fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack --port 3060",
    "build": "next build --turbopack",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:miniflare": "jest --config jest.miniflare.config.mjs",
    "test:miniflare:watch": "jest --config jest.miniflare.config.mjs --watch",
    "test:e2e": "jest --config jest.e2e.config.mjs",
    "test:e2e:watch": "jest --config jest.e2e.config.mjs --watch",
    "test:e2e:headful": "HEADLESS=false jest --config jest.e2e.config.mjs",
    "test:all": "pnpm test && pnpm test:miniflare && pnpm test:e2e",
    "lint": "eslint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky"
  },
  "dependencies": {
    "animejs": "4.1.3",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/puppeteer": "^7.0.4",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "19.1.0-rc.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "jest-puppeteer": "^11.0.0",
    "miniflare": "^4.20250906.2",
    "prettier": "^3.6.2",
    "puppeteer": "^24.21.0",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.1",
    "typescript": "^5"
  }
}
